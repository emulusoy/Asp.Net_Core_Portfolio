@using EntityLayer.Concrete
@model List<Experience>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Experience Page</h4>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Title</th>
                            <th>Date</th>
                            <th>Edit</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            
                       
                        <tr>
                            <td>@item.ExperienceID</td>
                            <td>@item.ExperienceName</td>
                            <td>@item.ExperienceDate</td>
                            <td>
                                <a class="btn btn-outline-danger" href="/Experience/DeleteExperience/@item.ExperienceID">Del</a>
                                    <a class="btn btn-outline-light" href="/Experience/EditExperience/@item.ExperienceID">Edit</a>
                                
                            </td>
                        </tr>
                        }
                    </tbody>
                </table>
                <a href="/Experience/AddExperience" class="btn btn-light">Add</a>
            </div>
        </div>
    </div>
</div>

